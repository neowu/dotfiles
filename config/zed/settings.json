{
  "theme": "Nord",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 14,
  "autosave": "on_focus_change",
  "inlay_hints": {
    "enabled": true
  },
  "wrap_guides": [120],
  "preferred_line_length": 120,
  "relative_line_numbers": true,
  "vim_mode": true,
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_multiline_find": true,
    "use_smartcase_find": true,
    "use_system_clipboard": "on_yank"
  },
  // "proxy": "socks5://dev.internal:1080",
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "enabled": false,
    "version": "2"
  },
  "terminal": {
    "copy_on_select": true,
    "detect_venv": "off"
  },
  "features": {
    "copilot": false,
    "inline_completion_provider": "none"
  },
  "chat_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "tabs": {
    "git_status": true
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "languages": {
    "JSON": {
      "preferred_line_length": 120
    },
    "Markdown": {
      "preferred_line_length": 300
    },
    "TOML": {
      "language_servers": ["!taplo"]
    }
  },
  "lsp": {
    "rust-analyzer": {
      "binary": {
        "path": "/Users/neo/.local/share/cargo/bin/rust-analyzer",
        "arguments": ["--no-log-buffering"]
      },
      "initialization_options": {
        "rustfmt": {
          "extraArgs": ["+nightly"]
        },
        "check": {
          "command": "clippy"
        }
      }
    }
  },
  "experimental.theme_overrides": {
    "editor.active_line.background": "#97acd633",
    "syntax": {
      "punctuation": {
        "color": "#81a1c1"
      },
      "punctuation.bracket": {
        "color": "#81a1c1"
      },
      "variable.parameter": {
        "color": "#d8dee9"
      },
      "attribute": {
        "color": "#d08770"
      }
    },
    "players": [
      {
        "selection": "#97acd633"
      }
    ]
  }
}
